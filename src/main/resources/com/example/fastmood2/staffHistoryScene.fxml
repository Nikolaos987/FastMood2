<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.fastmood2.StaffHistoryController">
   <children>
      <TableView fx:id="staffHistoryTable" layoutX="37.0" layoutY="43.0" prefHeight="241.0" prefWidth="531.0">
        <columns>
          <TableColumn fx:id="name" prefWidth="75.0" text="Name" />
          <TableColumn fx:id="tableNumber" prefWidth="75.0" text="Table Number" />
            <TableColumn fx:id="times" prefWidth="75.0" text="Times" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="filterButton" layoutX="363.0" layoutY="314.0" mnemonicParsing="false" onAction="#showHistory" text="O-" />
      <TextField fx:id="staffIDText" layoutX="206.0" layoutY="314.0" promptText="filter by Staff ID..." />
   </children>
</AnchorPane>
